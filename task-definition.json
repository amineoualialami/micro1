{
  "family": "micro1",
  "executionRoleArn": "arn:aws:iam::326646175787:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "xray-daemon",
      "image": "amazon/aws-xray-daemon",
      "cpu": 32,
      "memoryReservation": 256,
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 2000,
          "protocol": "udp"
        }
      ]
    },
    {
      "name": "micro1-container",
      "image": "326646175787.dkr.ecr.us-east-2.amazonaws.com/micro1:4655390a2d95f72f5f379cb62618a305641987ba",
      "cpu": 512,
      "environment": [
        { "name" : "AWS_XRAY_DAEMON_ADDRESS", "value" : "xray-daemon:2000" }
      ],
      "portMappings": [
        {
          "hostPort": 8888,
          "protocol": "tcp",
          "containerPort": 8888
        }
      ],
      "links": [
        "xray-daemon"
      ],
      "memory": 1024,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dcdojo",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}